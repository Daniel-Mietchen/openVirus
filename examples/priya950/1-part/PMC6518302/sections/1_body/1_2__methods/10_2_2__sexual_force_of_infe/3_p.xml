            <mi>S</mi>
           </msub>
          </mrow>
          <mrow>
           <mi>d</mi>
           <mi>t</mi>
          </mrow>
         </mfrac>
        </mstyle>
        <mo>=</mo>
        <mo>−</mo>
        <mrow>
         <mo>(</mo>
         <msub>
          <mi>λ</mi>
          <mrow>
           <mi>S</mi>
           <mi>W</mi>
          </mrow>
         </msub>
         <mo>+</mo>
         <msub>
          <mi>λ</mi>
          <mi>V</mi>
         </msub>
         <mo>)</mo>
        </mrow>
        <msub>
         <mi>W</mi>
         <mi>S</mi>
        </msub>
       </mrow>
      </mtd>
     </mtr>
    </mtable>
   </mrow>
  </math>
 </disp-formula>
