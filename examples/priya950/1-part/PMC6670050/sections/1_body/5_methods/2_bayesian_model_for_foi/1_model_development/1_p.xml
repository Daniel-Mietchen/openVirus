        <mo stretchy="true">(</mo>
        <mrow>
         <mi>a</mi>
         <mo>,</mo>
         <mi>y</mi>
        </mrow>
        <mo stretchy="true">)</mo>
       </mrow>
      </mrow>
      <mo>=</mo>
      <mn>1</mn>
      <mo>−</mo>
      <mrow>
       <mi>exp</mi>
      </mrow>
      <mrow>
       <mrow>
        <mo stretchy="true">{</mo>
