      <mrow>
       <mrow>
        <mo stretchy="true">{</mo>
        <mrow>
         <mo form="prefix">−</mo>
         <munderover>
          <mrow>
           <mo form="prefix">∑</mo>
          </mrow>
          <mrow>
           <msub>
            <mrow>
             <mi>t</mi>
            </mrow>
