           <mn>1</mn>
          </mrow>
          <mi>y</mi>
         </munderover>
         <mrow>
          <mrow>
           <mo stretchy="true">(</mo>
           <mrow>
            <msub>
             <mrow>
              <mi mathvariant="normal">λ</mi>
             </mrow>
             <mrow>
              <msub>
               <mrow>
                <mi>t</mi>
               </mrow>
               <mrow>
                <mn>2</mn>
               </mrow>
              </msub>
