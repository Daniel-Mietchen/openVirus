        <mrow>
         <mi>R</mi>
         <mi>e</mi>
         <mi>y</mi>
        </mrow>
       </msub>
      </mrow>
     </mfrac>
     <mo>=</mo>
     <mfrac>
      <mrow>
       <msub>
        <mi>D</mi>
        <mrow>
         <mi>L</mi>
         <mi>R</mi>
         <mi>G</mi>
         <mi>V</mi>
        </mrow>
       </msub>
       <msub>
