   <mo stretchy="false">(</mo>
   <mrow>
    <msub>
     <mi>a</mi>
     <mo>±</mo>
    </msub>
    <mo>,</mo>
    <mo> </mo>
    <msub>
     <mi>l</mi>
     <mrow>
      <mn>1</mn>
      <mo>,</mo>
      <mn>2</mn>
     </mrow>
    </msub>
   </mrow>
   <mo stretchy="false">)</mo>
   <mo>=</mo>
   <mrow>
    <mo>{</mo>
    <mrow>
     <mtable rowspacing="4pt" columnspacing="1em">
      <mtr>
       <mtd>
        <mrow>
         <msub>
