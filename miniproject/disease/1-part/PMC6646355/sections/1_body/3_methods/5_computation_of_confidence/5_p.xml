     <mo stretchy="true">(</mo>
     <mrow>
      <msub>
       <mrow>
        <mi mathvariant="normal">Φ</mi>
       </mrow>
       <mrow>
        <mi>j</mi>
       </mrow>
      </msub>
     </mrow>
     <mo stretchy="true">)</mo>
    </mrow>
    <mo>=</mo>
    <mfrac>
     <mrow>
      <mi>∂</mi>
      <msub>
       <mrow>
        <mi>P</mi>
       </mrow>
       <mrow>
        <msub>
