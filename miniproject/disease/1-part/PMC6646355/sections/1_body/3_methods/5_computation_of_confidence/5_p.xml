     <mi>h</mi>
    </mfrac>
    <mo>+</mo>
    <mi>O</mi>
    <mo stretchy="false">(</mo>
    <msup>
     <mrow>
      <mi>h</mi>
     </mrow>
     <mn>2</mn>
    </msup>
    <mo stretchy="false">)</mo>
    <mo>,</mo>
