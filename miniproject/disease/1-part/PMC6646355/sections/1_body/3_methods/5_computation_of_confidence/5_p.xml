      </msub>
      <mspace width=".25em"/>
     </mrow>
     <mrow>
      <mi>∂</mi>
      <msub>
       <mrow>
        <mi>Φ</mi>
       </mrow>
       <mrow>
        <mi>j</mi>
       </mrow>
      </msub>
     </mrow>
    </mfrac>
    <mo>≈</mo>
    <mfrac>
     <mrow>
      <mi>I</mi>
      <mi>m</mi>
      <mo stretchy="false">[</mo>
      <msub>
       <mrow>
        <mi>P</mi>
       </mrow>
       <mrow>
        <msub>
