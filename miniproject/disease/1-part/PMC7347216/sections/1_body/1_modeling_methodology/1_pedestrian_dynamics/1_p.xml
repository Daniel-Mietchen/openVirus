     <mrow>
      <mi>τ</mi>
     </mrow>
    </mfrac>
    <mo>(</mo>
    <mrow>
     <msubsup>
      <mrow>
       <mover accent="true">
        <mrow>
         <mi>v</mi>
        </mrow>
