     <mi>c</mi>
     <mi>a</mi>
     <mi>l</mi>
     <mi>e</mi>
     <mi>d</mi>
    </mrow>
   </msubsup>
   <mo>=</mo>
   <mfrac>
    <mrow>
     <msub>
      <mi>X</mi>
      <mi>i</mi>
     </msub>
     <mo>-</mo>
     <mi>m</mi>
     <mi>i</mi>
     <mi>n</mi>
     <mfenced open="(" close=")">
      <mrow>
       <mfenced open="{" close="}">
        <mi>X</mi>
       </mfenced>
      </mrow>
     </mfenced>
    </mrow>
    <mrow>
     <mrow>
      <mi>max</mi>
      <mrow>
       <mfenced open="(" close=")">
        <mrow>
         <mfenced open="{" close="}">
          <mi>X</mi>
         </mfenced>
        </mrow>
       </mfenced>
      </mrow>
     </mrow>
     <mo>-</mo>
     <mi>m</mi>
     <mi>i</mi>
     <mi>n</mi>
     <mfenced open="(" close=")">
      <mrow>
       <mfenced open="{" close="}">
        <mi>X</mi>
       </mfenced>
      </mrow>
     </mfenced>
    </mrow>
   </mfrac>
  </math>
 </inline-formula>. As such,
 <disp-formula id="eq0025">
  <math id="M21" altimg="si34.gif" overflow="scroll">
   <msub>
    <mi>f</mi>
    <mi>g</mi>
   </msub>
