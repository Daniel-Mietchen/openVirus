            </mrow>
           </msub>
           <mfenced open="(" close=")">
            <mn>2</mn>
           </mfenced>
           <mo>…</mo>
           <msub>
            <mi>λ</mi>
            <mrow>
             <mi>H</mi>
             <mi>H</mi>
            </mrow>
           </msub>
           <mfenced open="(" close=")">
            <mrow>
             <msub>
              <mi>t</mi>
              <mi>w</mi>
             </msub>
            </mrow>
           </mfenced>
