    </mrow>
    <mrow>
     <mrow>
      <mi>max</mi>
      <mrow>
       <mfenced open="(" close=")">
        <mrow>
         <mfenced open="{" close="}">
          <mrow>
           <msub>
            <mi>λ</mi>
            <mrow>
             <mi>H</mi>
             <mi>H</mi>
            </mrow>
           </msub>
           <mfenced open="(" close=")">
            <mn>1</mn>
           </mfenced>
