    <mrow>
     <msub>
      <mi>t</mi>
      <mi>w</mi>
     </msub>
    </mrow>
   </mfenced>
   <mo>=</mo>
   <mfrac>
    <mrow>
     <msub>
      <mi>λ</mi>
      <mrow>
       <mi>H</mi>
       <mi>H</mi>
      </mrow>
     </msub>
     <mfenced open="(" close=")">
      <mrow>
       <msub>
        <mi>t</mi>
        <mi>w</mi>
       </msub>
      </mrow>
     </mfenced>
     <mo>-</mo>
     <mi>m</mi>
     <mi>i</mi>
     <mi>n</mi>
     <mfenced open="(" close=")">
      <mrow>
       <mfenced open="{" close="}">
        <mrow>
         <msub>
          <mi>λ</mi>
          <mrow>
           <mi>H</mi>
           <mi>H</mi>
          </mrow>
         </msub>
         <mfenced open="(" close=")">
          <mn>1</mn>
         </mfenced>
         <mo>,</mo>
         <msub>
