     <xref rid="R11" ref-type="bibr">11</xref>
     <break/>
     <italic>J Formosan Med Assoc</italic>
    </td>
    <td align="left" valign="top" rowspan="1" colspan="1">Systematic review of nine case series</td>
    <td align="char" char="." valign="top" rowspan="1" colspan="1">26 February</td>
    <td align="left" valign="top" rowspan="1" colspan="1">China</td>
    <td align="left" valign="top" rowspan="1" colspan="1"> </td>
    <td align="left" valign="top" rowspan="1" colspan="1">93 cases</td>
    <td align="left" valign="top" rowspan="1" colspan="1">26% asymptomatics</td>
    <td align="left" valign="top" rowspan="1" colspan="1">75% family contact</td>
   </tr>
