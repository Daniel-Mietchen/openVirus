        </mrow>
        <mrow>
         <mo linebreak="badbreak">∗</mo>
        </mrow>
       </msup>
       <mo id="mmlalignd1e5303" linebreak="goodbreak" indentalign="id" indenttarget="mmlalignd1e5272">=</mo>
       <mfrac>
        <mrow>
         <mi>μ</mi>
         <mi>Λ</mi>
         <mrow>
          <mo>(</mo>
          <mi>μ</mi>
          <mo linebreak="badbreak">+</mo>
          <mi>γ</mi>
          <mo linebreak="badbreak">+</mo>
          <mi>Δ</mi>
          <mo>)</mo>
         </mrow>
         <mrow>
          <mo>(</mo>
          <mi>μ</mi>
          <mo linebreak="badbreak">+</mo>
          <mi>ξ</mi>
          <mo>)</mo>
         </mrow>
         <mrow>
          <mo>(</mo>
          <msub>
           <mrow>
            <mi>R</mi>
           </mrow>
