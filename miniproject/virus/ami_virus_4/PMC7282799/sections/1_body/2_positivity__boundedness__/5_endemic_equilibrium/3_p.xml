       <msup>
        <mrow>
         <mi>S</mi>
        </mrow>
        <mrow>
         <mo linebreak="badbreak">∗</mo>
        </mrow>
       </msup>
       <mo id="mmlalignd1e5272" linebreak="goodbreak">=</mo>
       <mfrac>
        <mrow>
         <mi>Λ</mi>
        </mrow>
        <mrow>
         <mi>μ</mi>
         <msub>
          <mrow>
           <mi>R</mi>
          </mrow>
          <mrow>
           <mn>0</mn>
          </mrow>
         </msub>
        </mrow>
       </mfrac>
       <mo>,</mo>
      </mrow>
     </math>
    </disp-formula>
