            <mn>0</mn>
           </mrow>
          </msub>
          <mo linebreak="badbreak">−</mo>
          <mn>1</mn>
          <mo>)</mo>
         </mrow>
        </mrow>
        <mrow>
         <mi>α</mi>
         <mi>β</mi>
         <mi>Λ</mi>
         <mrow>
          <mo>(</mo>
          <mn>1</mn>
          <mo linebreak="badbreak">−</mo>
          <mi>σ</mi>
          <mo>)</mo>
         </mrow>
         <mrow>
          <mo>(</mo>
          <mi>μ</mi>
          <mo linebreak="badbreak">+</mo>
          <mi>ξ</mi>
          <mo>)</mo>
         </mrow>
         <mo linebreak="badbreak">−</mo>
         <mi>α</mi>
         <mi>μ</mi>
         <mi>γ</mi>
         <mi>ξ</mi>
