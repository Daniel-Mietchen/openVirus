       <mi>S</mi>
       <mrow>
        <mo>(</mo>
        <mi>t</mi>
        <mo>)</mo>
       </mrow>
      </mrow>
      <mrow>
       <mi>d</mi>
       <mi>t</mi>
      </mrow>
     </mfrac>
     <mo id="mmlalignd1e6881" linebreak="goodbreak">=</mo>
     <mo linebreak="goodbreak">−</mo>
     <mi>β</mi>
