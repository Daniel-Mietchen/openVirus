<p>
 <inline-formula id="IEq2">
  <alternatives>
   <tex-math id="M13">\documentclass[12pt]{minimal} \usepackage{amsmath} \usepackage{wasysym} \usepackage{amsfonts} \usepackage{amssymb} \usepackage{amsbsy} \usepackage{mathrsfs} \usepackage{upgreek} \setlength{\oddsidemargin}{-69pt} \begin{document}$$ \mathrm{Let}\kern0.24em \tilde{R}=\frac{\beta_0}{\mu_0}. $$\end{document}</tex-math>
   <math id="M14">
    <mrow>
     <mi>Let</mi>
     <mspace width="0.24em"/>
     <mover accent="true">
      <mi>R</mi>
      <mo stretchy="true">˜</mo>
     </mover>
     <mo>=</mo>
     <mfrac>
      <msub>
       <mi>β</mi>
       <mn>0</mn>
      </msub>
      <msub>
       <mi>μ</mi>
       <mn>0</mn>
