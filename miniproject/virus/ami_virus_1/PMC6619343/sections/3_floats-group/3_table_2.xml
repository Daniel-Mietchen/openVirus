   <tr>
    <td align="left" style="padding-left:10%" rowspan="1" colspan="1">θ</td>
    <td align="left" rowspan="1" colspan="1">Aphid dispersal</td>
    <td align="left" rowspan="1" colspan="1">Rate per day</td>
   </tr>
