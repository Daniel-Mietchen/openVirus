    <td align="left" style="padding-left:10%" rowspan="1" colspan="1">
     <italic>b</italic>
    </td>
    <td align="left" rowspan="1" colspan="1">Aphid mortality</td>
    <td align="left" rowspan="1" colspan="1">Rate per day</td>
