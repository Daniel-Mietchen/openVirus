     <td align="left" rowspan="1" colspan="1">2 (1.5 SD)</td>
     <td align="left" rowspan="1" colspan="1">Gamma</td>
     <td align="left" rowspan="1" colspan="1">[
      <xref rid="pntd.0007988.ref019" ref-type="bibr">19</xref>–
      <xref rid="pntd.0007988.ref021" ref-type="bibr">21</xref>]
