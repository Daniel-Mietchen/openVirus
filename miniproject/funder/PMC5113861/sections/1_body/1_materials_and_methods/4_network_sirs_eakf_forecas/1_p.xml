        </mrow>
        <mrow>
         <msubsup>
          <mi>σ</mi>
          <mrow>
           <mi>k</mi>
           <mo>,</mo>
           <mi>o</mi>
           <mi>b</mi>
           <mi>s</mi>
          </mrow>
          <mn>2</mn>
         </msubsup>
         <mo>+</mo>
         <msubsup>
          <mi>σ</mi>
          <mrow>
           <mi>k</mi>
           <mo>,</mo>
           <mi>p</mi>
           <mi>r</mi>
           <mi>i</mi>
           <mi>o</mi>
           <mi>r</mi>
          </mrow>
          <mn>2</mn>
         </msubsup>
        </mrow>
       </mfrac>
      </mrow>
     </msqrt>
     <mo stretchy="false">(</mo>
     <msubsup>
      <mi>x</mi>
      <mrow>
       <mi>k</mi>
       <mo>,</mo>
       <mi>p</mi>
       <mi>r</mi>
       <mi>i</mi>
       <mi>o</mi>
       <mi>r</mi>
      </mrow>
      <mi>m</mi>
     </msubsup>
     <mo>−</mo>
     <msub>
      <mover accent="true">
       <mi>x</mi>
       <mo>¯</mo>
      </mover>
      <mrow>
       <mi>k</mi>
       <mo>,</mo>
       <mi>p</mi>
       <mi>r</mi>
       <mi>i</mi>
       <mi>o</mi>
       <mi>r</mi>
      </mrow>
     </msub>
     <mo stretchy="false">)</mo>
    </mrow>
   </math>
  </alternatives>
  <label>(6)</label>
 </disp-formula>
