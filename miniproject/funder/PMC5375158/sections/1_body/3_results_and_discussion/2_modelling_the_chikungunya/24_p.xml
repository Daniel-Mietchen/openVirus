   <math id="M24">
    <mi mathvariant="script">H</mi>
   </math>
  </alternatives>
 </inline-formula>, 
 <italic>i.e.</italic>
 <inline-formula id="pone.0174293.e025">
  <alternatives>
   <graphic xlink:href="pone.0174293.e025.jpg" id="pone.0174293.e025g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
   <math id="M25">
