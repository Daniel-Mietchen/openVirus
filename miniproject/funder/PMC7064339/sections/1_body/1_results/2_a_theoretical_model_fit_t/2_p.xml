        <mrow>
         <mi>S</mi>
        </mrow>
       </msub>
      </mrow>
      <mrow>
       <mi>d</mi>
       <mi>t</mi>
      </mrow>
     </mfrac>
     <mo>=</mo>
     <mi>b</mi>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>D</mi>
      </mrow>
     </msub>
     <mrow>
      <mo>(</mo>
      <mrow>
       <msub>
        <mi>P</mi>
        <mrow>
         <mi>S</mi>
        </mrow>
       </msub>
       <mo>+</mo>
       <mtext> </mtext>
       <msub>
        <mi>P</mi>
        <mrow>
         <mi>A</mi>
        </mrow>
       </msub>
      </mrow>
      <mo>)</mo>
     </mrow>
     <mo>−</mo>
     <mi>β</mi>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>S</mi>
      </mrow>
     </msub>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>I</mi>
      </mrow>
     </msub>
     <mo>−</mo>
     <mi>μ</mi>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>S</mi>
      </mrow>
     </msub>
     <mo>−</mo>
     <mi>ρ</mi>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>E</mi>
      </mrow>
     </msub>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>S</mi>
      </mrow>
     </msub>
     <mo>−</mo>
     <mtext> </mtext>
     <mi>ε</mi>
     <msub>
      <mi>P</mi>
      <mrow>
       <mi>S</mi>
      </mrow>
