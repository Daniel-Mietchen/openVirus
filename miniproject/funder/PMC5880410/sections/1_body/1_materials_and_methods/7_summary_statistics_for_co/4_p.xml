      <mrow>
       <mn>0</mn>
      </mrow>
      <mi>∞</mi>
     </msubsup>
     <mspace width="4pt"/>
     <mo>=</mo>
     <mspace width="4pt"/>
     <mfrac>
      <mrow>
       <msubsup>
        <mo>∑</mo>
        <mrow>
         <mi>i</mi>
         <mo>=</mo>
         <mn>1</mn>
        </mrow>
        <mi>m</mi>
       </msubsup>
       <msub>
        <mi>β</mi>
        <mi>i</mi>
       </msub>
       <msub>
        <mi>N</mi>
        <mi>i</mi>
       </msub>
      </mrow>
      <mi>γ</mi>
     </mfrac>
     <mi>a</mi>
     <mrow>
      <mo>(</mo>
      <mn>0</mn>
      <mo>)</mo>
     </mrow>
     <mo>,</mo>
    </mrow>
   </math>
