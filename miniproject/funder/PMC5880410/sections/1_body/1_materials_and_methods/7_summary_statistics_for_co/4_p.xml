   <math id="M24">
    <msub>
     <mi>R</mi>
     <mn>0</mn>
    </msub>
    <mrow>
     <mo>=</mo>
     <munder>
      <mtext>lim</mtext>
      <mrow>
       <mi>t</mi>
       <mo>→</mo>
       <mi>∞</mi>
      </mrow>
     </munder>
     <msup>
      <mi>I</mi>
      <mrow>
       <mo>(</mo>
       <mn>1</mn>
       <mo>)</mo>
      </mrow>
     </msup>
     <mrow>
      <mo>(</mo>
      <mi>t</mi>
      <mo>)</mo>
     </mrow>
     <mspace width="4pt"/>
     <mo>=</mo>
     <mspace width="4pt"/>
     <mi>a</mi>
     <mrow>
      <mo>(</mo>
      <mn>0</mn>
      <mo>)</mo>
     </mrow>
     <munderover>
      <mo>∑</mo>
      <mrow>
       <mi>i</mi>
       <mo>=</mo>
       <mn>1</mn>
      </mrow>
      <mi>m</mi>
     </munderover>
     <msub>
      <mi>β</mi>
      <mi>i</mi>
     </msub>
     <msub>
      <mi>N</mi>
      <mi>i</mi>
     </msub>
     <mo>[</mo>
     <mfrac>
      <msup>
       <mi>e</mi>
       <mrow>
        <mo>−</mo>
        <mi>γ</mi>
        <mi>t</mi>
       </mrow>
      </msup>
      <mrow>
       <mo>−</mo>
       <mi>γ</mi>
      </mrow>
