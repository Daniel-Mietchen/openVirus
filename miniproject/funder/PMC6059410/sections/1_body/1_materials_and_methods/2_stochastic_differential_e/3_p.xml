    <mrow>
     <mi>δ</mi>
     <mi>U</mi>
     <mo>=</mo>
     <msub>
      <mi>q</mi>
      <mi>d</mi>
     </msub>
     <mi>D</mi>
     <mo>+</mo>
     <mi>ϵ</mi>
     <mi>D</mi>
     <mo>=</mo>
     <mfrac>
      <mrow>
       <mn>0</mn>
       <mo>.</mo>
       <mn>046</mn>
       <msub>
        <mi>D</mi>
        <mn>2005</mn>
