      <mo>…</mo>
      <mo>,</mo>
      <msub>
       <mi>n</mi>
       <mn>9</mn>
      </msub>
      <mo>)</mo>
     </mrow>
     <mo>∝</mo>
     <msubsup>
      <mo>Π</mo>
      <mrow>
       <mi>t</mi>
       <mo>=</mo>
       <mn>1</mn>
      </mrow>
      <mn>9</mn>
     </msubsup>
     <msubsup>
      <mi>p</mi>
      <mrow>
