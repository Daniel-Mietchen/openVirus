         <mrow>
          <mi>i</mi>
          <mo>=</mo>
          <mn>1</mn>
         </mrow>
         <mi>K</mi>
        </munderover>
        <msub>
         <mi>β</mi>
         <mi>i</mi>
        </msub>
        <msub>
         <mi>X</mi>
         <mrow>
          <mi>i</mi>
          <mo>,</mo>
          <mi>t</mi>
         </mrow>
        </msub>
        <mo>+</mo>
        <msub>
         <mi>ε</mi>
         <mi>t</mi>
        </msub>
        <mo>,</mo>
        <mspace width="5.0pt"/>
        <msub>
         <mi>ε</mi>
         <mi>t</mi>
        </msub>
        <msup>
         <mo/>
