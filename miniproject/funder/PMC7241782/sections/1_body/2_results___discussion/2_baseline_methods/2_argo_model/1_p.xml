        </msub>
        <mo>=</mo>
        <msub>
         <mi>μ</mi>
         <mi>y</mi>
        </msub>
        <mo>+</mo>
        <munderover>
         <mo>∑</mo>
         <mrow>
          <mi>j</mi>
          <mo>=</mo>
          <mn>1</mn>
         </mrow>
         <mi>N</mi>
        </munderover>
        <msub>
         <mi>α</mi>
         <mi>j</mi>
        </msub>
        <msub>
         <mi>y</mi>
         <mrow>
          <mi>t</mi>
          <mo>-</mo>
          <mi>j</mi>
         </mrow>
        </msub>
        <mo>+</mo>
        <munderover>
