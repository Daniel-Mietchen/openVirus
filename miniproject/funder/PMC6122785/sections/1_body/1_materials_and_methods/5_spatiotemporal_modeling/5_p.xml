 <inline-formula id="pone.0202832.e015">
  <alternatives>
   <graphic xlink:href="pone.0202832.e015.jpg" id="pone.0202832.e015g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
