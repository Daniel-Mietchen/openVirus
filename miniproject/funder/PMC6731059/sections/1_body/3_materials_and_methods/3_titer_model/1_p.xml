      </mrow>
      <mo>-</mo>
      <mrow>
       <msub>
        <mi>log</mi>
        <mn>2</mn>
       </msub>
       <mo>⁢</mo>
       <mrow>
        <mo stretchy="false">(</mo>
        <msub>
         <mi>T</mi>
         <mrow>
          <mi>i</mi>
