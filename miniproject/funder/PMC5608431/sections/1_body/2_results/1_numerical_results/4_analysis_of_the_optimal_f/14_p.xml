 </inline-formula> and 
 <inline-formula id="pcbi.1005696.e034">
  <alternatives>
   <graphic xlink:href="pcbi.1005696.e034.jpg" id="pcbi.1005696.e034g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
   <math id="M34">
