 <inline-formula id="pcbi.1005696.e023">
  <alternatives>
   <graphic xlink:href="pcbi.1005696.e023.jpg" id="pcbi.1005696.e023g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
