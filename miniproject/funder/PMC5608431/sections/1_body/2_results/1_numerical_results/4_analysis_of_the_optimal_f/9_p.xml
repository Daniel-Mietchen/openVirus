 </inline-formula> satisfies 
 <disp-formula id="pcbi.1005696.e019">
  <alternatives>
   <graphic xlink:href="pcbi.1005696.e019.jpg" id="pcbi.1005696.e019g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
