  </alternatives>
 </inline-formula> satisfies 
 <disp-formula id="pcbi.1005696.e014">
  <alternatives>
   <graphic xlink:href="pcbi.1005696.e014.jpg" id="pcbi.1005696.e014g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
   <math id="M14">
    <mtable displaystyle="true">
     <mtr>
      <mtd columnalign="right">
       <mrow>
        <msup>
         <mo>Δ</mo>
         <mo>′</mo>
        </msup>
        <mrow>
         <mo>(</mo>
         <mi>f</mi>
         <mo>)</mo>
        </mrow>
        <mo>=</mo>
        <msub>
         <mi>c</mi>
