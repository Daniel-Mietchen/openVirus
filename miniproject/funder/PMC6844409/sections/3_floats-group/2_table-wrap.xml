  <thead>
   <tr>
    <th rowspan="1" colspan="1">Biomarker</th>
    <th rowspan="1" colspan="1">What it is</th>
    <th rowspan="1" colspan="1">What it means</th>
   </tr>
  </thead>
  <tbody>
   <tr>
    <td rowspan="1" colspan="1">HBsAg</td>
    <td rowspan="1" colspan="1">Viral surface protein</td>
    <td rowspan="1" colspan="1">HBV infection or transcriptionally active HBV integration</td>
   </tr>
   <tr>
    <td rowspan="1" colspan="1">HBsAb or anti-HBs</td>
    <td rowspan="1" colspan="1">Antibodies to HBsAg</td>
    <td rowspan="1" colspan="1">Effective immune response following infection or HBV vaccination</td>
   </tr>
   <tr>
    <td rowspan="1" colspan="1">HBcAg</td>
    <td rowspan="1" colspan="1">Viral capsid (core) protein</td>
    <td rowspan="1" colspan="1">HBV infection</td>
   </tr>
   <tr>
    <td rowspan="1" colspan="1">HBcAb or anti-HBc</td>
    <td rowspan="1" colspan="1">Antibodies to HBcAg</td>
    <td rowspan="1" colspan="1">Past or present HBV infection</td>
   </tr>
   <tr>
    <td rowspan="1" colspan="1">HBeAg</td>
    <td rowspan="1" colspan="1">Secreted variant of HBcAg</td>
    <td rowspan="1" colspan="1">High HBV replication</td>
   </tr>
   <tr>
    <td rowspan="1" colspan="1">HBeAb or anti-HBe</td>
    <td rowspan="1" colspan="1">Antibodies to HBeAg</td>
    <td rowspan="1" colspan="1">Low HBV replication</td>
   </tr>
   <tr>
    <td rowspan="1" colspan="1">HBV DNA</td>
    <td rowspan="1" colspan="1">Viral genome in virus</td>
    <td rowspan="1" colspan="1">Active viral replication</td>
   </tr>
