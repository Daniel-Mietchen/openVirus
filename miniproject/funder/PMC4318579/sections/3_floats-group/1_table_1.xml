     <td align="left" rowspan="1" colspan="1">400
      <xref rid="t001fn002" ref-type="table-fn">
       <sup>b</sup>
      </xref> [
      <xref rid="pcbi.1004093.ref072" ref-type="bibr">72</xref>–
      <xref rid="pcbi.1004093.ref074" ref-type="bibr">74</xref>]
     </td>
    </tr>
    <tr>
     <td align="left" rowspan="1" colspan="1">κ
      <sub>min</sub>
     </td>
