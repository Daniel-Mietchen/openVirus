     <td align="left" rowspan="1" colspan="1">1</td>
    </tr>
    <tr>
     <td align="left" rowspan="1" colspan="1">κ
      <sub>max</sub>
     </td>
     <td align="left" rowspan="1" colspan="1">Upper cutoff of annual degree distribution</td>
     <td align="left" rowspan="1" colspan="1">1000
      <xref rid="t001fn003" ref-type="table-fn">
       <sup>c</sup>
      </xref>
     </td>
    </tr>
    <tr>
     <td align="left" rowspan="1" colspan="1">p
      <sub>b</sub>
     </td>
