            <mi>j</mi>
           </mrow>
          </msub>
          <msub>
           <mi>R</mi>
           <mi>cap</mi>
          </msub>
         </mfrac>
        </mfenced>
        <mo>−</mo>
        <mi>γ</mi>
        <msub>
         <mi>R</mi>
         <mrow>
          <mi>i</mi>
          <mo>,</mo>
          <mi>j</mi>
         </mrow>
        </msub>
        <mo>−</mo>
        <mi>ρ</mi>
        <msub>
         <mi>R</mi>
         <mrow>
          <mi>i</mi>
          <mo>,</mo>
          <mi>j</mi>
         </mrow>
        </msub>
       </mrow>
      </mtd>
     </mtr>
     <mtr>
      <mtd columnalign="right">
       <mfrac>
