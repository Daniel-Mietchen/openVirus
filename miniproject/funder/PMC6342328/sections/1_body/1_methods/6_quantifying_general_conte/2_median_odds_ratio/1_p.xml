   <math id="M6">
    <msub>
     <mrow>
      <mi mathvariant="normal">M</mi>
      <mi mathvariant="normal">O</mi>
      <mi mathvariant="normal">R</mi>
     </mrow>
     <mrow>
      <mi>H</mi>
     </mrow>
    </msub>
    <mo>≈</mo>
    <mi mathvariant="normal">e</mi>
    <mi mathvariant="normal">x</mi>
    <mi mathvariant="normal">p</mi>
    <mo>(</mo>
    <mn>0.95</mn>
    <mo>*</mo>
    <msqrt>
     <mrow>
      <msup>
       <mi>σ</mi>
       <mn>2</mn>
      </msup>
      <msub>
       <mrow/>
       <mi>C</mi>
      </msub>
      <mo>+</mo>
      <msup>
       <mi>σ</mi>
       <mn>2</mn>
      </msup>
      <msub>
       <mrow/>
       <mrow>
        <mi>S</mi>
        <mi>L</mi>
       </mrow>
      </msub>
      <mo>+</mo>
      <msup>
       <mi>σ</mi>
       <mn>2</mn>
      </msup>
      <msub>
       <mrow/>
       <mi>H</mi>
      </msub>
     </mrow>
    </msqrt>
    <mo>)</mo>
   </math>
  </alternatives>
 </disp-formula>
