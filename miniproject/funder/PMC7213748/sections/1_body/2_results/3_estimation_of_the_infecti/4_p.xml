     <mi mathvariant="script">R</mi>
     <mrow>
      <mi>v</mi>
     </mrow>
     <mn>2</mn>
    </msubsup>
   </math>
  </alternatives>
 </inline-formula> (i.e., 
 <inline-formula id="pntd.0007502.e047">
  <alternatives>
   <graphic xlink:href="pntd.0007502.e047.jpg" id="pntd.0007502.e047g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
