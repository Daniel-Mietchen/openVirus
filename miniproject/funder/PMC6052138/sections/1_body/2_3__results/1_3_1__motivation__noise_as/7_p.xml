        </msup>
       </mrow>
       <mrow>
        <msqrt>
         <mrow>
          <mn>2</mn>
          <mi>π</mi>
          <msubsup>
           <mrow>
            <mi>σ</mi>
           </mrow>
           <mrow>
            <mi>A</mi>
           </mrow>
           <mrow>
            <mn>2</mn>
           </mrow>
          </msubsup>
         </mrow>
        </msqrt>
       </mrow>
      </mfrac>
      <mo>,</mo>
      <mtext> </mtext>
      <mi>I</mi>
      <mo>=</mo>
      <mn>1</mn>
      <mo>,</mo>
      <mn>2</mn>
      <mo>,</mo>
      <mo>…</mo>
