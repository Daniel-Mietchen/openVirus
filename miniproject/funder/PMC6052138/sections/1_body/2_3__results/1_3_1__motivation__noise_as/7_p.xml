         <mrow>
          <mi>A</mi>
         </mrow>
        </msub>
        <mo>,</mo>
        <msub>
         <mrow>
          <mi>σ</mi>
         </mrow>
         <mrow>
          <mi>A</mi>
         </mrow>
        </msub>
        <mo>,</mo>
        <mi>t</mi>
       </mrow>
       <mo stretchy="false">)</mo>
      </mrow>
      <mo>=</mo>
      <mi>P</mi>
      <mrow>
       <mo stretchy="false">(</mo>
       <mrow>
        <mi>I</mi>
        <mo>|</mo>
        <mi>α</mi>
