           <mo stretchy="false">)</mo>
          </mrow>
         </mrow>
         <mrow>
          <mi>I</mi>
          <mo>-</mo>
          <mn>1</mn>
         </mrow>
        </msup>
       </mrow>
       <mo>]</mo>
      </mrow>
      <mfrac>
       <mrow>
        <msup>
         <mrow>
          <mi>e</mi>
         </mrow>
         <mrow>
          <mo>-</mo>
          <msup>
           <mrow>
            <mrow>
             <mo stretchy="false">(</mo>
