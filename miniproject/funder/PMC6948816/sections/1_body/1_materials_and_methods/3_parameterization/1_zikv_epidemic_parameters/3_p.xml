   <math id="M4">
    <mtable displaystyle="true">
     <mtr>
      <mtd columnalign="right">
       <mrow>
        <mfrac>
         <mrow>
          <mi>d</mi>
          <mi>C</mi>
         </mrow>
         <mrow>
          <mi>d</mi>
          <mi>t</mi>
         </mrow>
        </mfrac>
        <mo>=</mo>
        <munder>
         <mo>∑</mo>
         <mi>a</mi>
        </munder>
        <msub>
         <mi>β</mi>
         <mi>a</mi>
        </msub>
        <msub>
         <mi>S</mi>
         <mi>a</mi>
        </msub>
