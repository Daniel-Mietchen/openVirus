  <label>(12)</label>
 </disp-formula>
 <disp-formula id="pcbi.1007518.e027">
  <alternatives>
   <graphic xlink:href="pcbi.1007518.e027.jpg" id="pcbi.1007518.e027g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
   <math id="M27">
    <mtable displaystyle="true">
     <mtr>
      <mtd columnalign="right">
       <mrow>
