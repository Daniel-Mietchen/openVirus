         <mover accent="true">
          <mi>Y</mi>
          <mo>˜</mo>
         </mover>
         <mi>t</mi>
        </msub>
        <mo>=</mo>
        <mover accent="true">
         <mi>C</mi>
         <mo>˜</mo>
        </mover>
        <msub>
         <mover accent="true">
          <mi>X</mi>
          <mo>˜</mo>
         </mover>
         <mi>t</mi>
        </msub>
