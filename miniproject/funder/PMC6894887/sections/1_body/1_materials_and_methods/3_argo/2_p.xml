        <munder>
         <mo>∑</mo>
         <mrow>
          <mi>j</mi>
          <mo>∈</mo>
          <mi>J</mi>
         </mrow>
        </munder>
        <msub>
         <mi>q</mi>
         <mi>j</mi>
        </msub>
        <msub>
         <mi>z</mi>
         <mrow>
          <mi>t</mi>
          <mo>,</mo>
          <mi>j</mi>
         </mrow>
        </msub>
        <msup>
         <mo>)</mo>
         <mn>2</mn>
        </msup>
       </mrow>
      </mtd>
     </mtr>
     <mtr>
      <mtd columnalign="right">
       <mrow>
        <mo>+</mo>
        <munder>
         <mo>∑</mo>
         <mrow>
          <mi>m</mi>
          <mo>∈</mo>
          <mi>M</mi>
         </mrow>
        </munder>
        <msub>
         <mo>λ</mo>
         <msub>
          <mi>a</mi>
          <mi>m</mi>
         </msub>
        </msub>
        <mrow>
         <mo>|</mo>
        </mrow>
        <msub>
         <mi>a</mi>
         <mi>m</mi>
        </msub>
        <mrow>
         <mo>|</mo>
         <mo>+</mo>
        </mrow>
        <munder>
         <mo>∑</mo>
         <mrow>
          <mi>g</mi>
          <mo>∈</mo>
          <mi>G</mi>
