   </math>
  </alternatives>
 </disp-formula> where 
 <italic>t</italic> is the time series index, 
 <italic>Y(t)</italic> is the observed case count on that index day, 
 <inline-formula id="pone.0191324.e002">
  <alternatives>
   <graphic xlink:href="pone.0191324.e002.jpg" id="pone.0191324.e002g" mimetype="image" position="anchor" orientation="portrait" xmlns:xlink="http://www.w3.org/1999/xlink"/>
