   <math id="M1">
    <mi>W</mi>
    <mn>2</mn>
    <mi>c</mi>
    <mo>(</mo>
    <mi>t</mi>
    <mo>)</mo>
    <mo>=</mo>
    <mfrac>
     <mrow>
      <mi>Y</mi>
      <mo>(</mo>
      <mrow>
       <mi>t</mi>
      </mrow>
      <mo>)</mo>
      <mo>−</mo>
      <mover accent="true">
       <mrow>
        <mi>Y</mi>
       </mrow>
       <mo>−</mo>
      </mover>
      <mo>(</mo>
      <mi>t</mi>
      <mo>)</mo>
     </mrow>
     <mrow>
