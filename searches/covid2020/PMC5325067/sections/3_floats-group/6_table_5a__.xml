    <td rowspan="1" colspan="1">China (55% 
     <break/>rural)
    </td>
    <td rowspan="1" colspan="1">71,785,000</td>
    <td rowspan="1" colspan="1">14%</td>
    <td rowspan="1" colspan="1">10</td>
    <td rowspan="1" colspan="1">13</td>
